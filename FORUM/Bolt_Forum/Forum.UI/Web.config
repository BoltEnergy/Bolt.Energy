    <defaultDocument>
      <files>
        <add value="/Login.aspx"/>
      </files>
    </defaultDocument>
    <modules>
      <remove name="UrlRewrite"/>
      <remove name="SessionIdDomain"/>
      <add name="UrlRewrite" type="Com.Comm100.Forum.UI.UrlRewrite,Forum.UI"/>
      <add name="SessionIdDomain" type="Com.Comm100.Framework.SessionControl.SessionIdDomain,Framework"/>
    </modules>
    <validation validateIntegratedModeConfiguration ="false"/>
  </system.webServer>
  <system.net>
